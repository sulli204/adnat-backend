<%= stylesheet_link_tag "table.css" %>
<h2><%= organization.name ></h2>

<h3>Shifts</h3>
<table>
    <thead>
        <tr>
            <th>Employee Name</th>
            <th>Shift Date</th>
            <th>Start Time</th>
            <th>Finish Time</th>
            <th>Break Length</th>
            <th>Hours Worked</th>
            <th>Shift Cost</th>
        </tr>
    </thead>

    <tbody>
        <% @shifts.each do |shift| %>
            
    </tbody>
</table>